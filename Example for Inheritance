package day1;
class CreditCard //parent class
{
	int CardNo = 1234567;
	int pin = 3003;
}
class Hacker extends CreditCard // child class 
{
	void viewDetails()
	{
		System.out.println("Card Number : " +CardNo);
		System.out.println("pin number :" +pin);
	}
	void changeDetails()
	{
		int CardNo = 7654321;
		int pin = 2210;
	}
	
}

public class ExampleForInheritance {

	public static void main(String[] args) {
Hacker h = new Hacker();
h.viewDetails();
h.changeDetails();
		

	}

}
output
Card Number : 1234567
pin number :3003
